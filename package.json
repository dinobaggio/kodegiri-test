{
  "name": "kodegiri-test",
  "version": "1.0.0",
  "description": "kodegiri test interview",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "sh build",
    "serve": "node ./dist/bin/server.js"
  },
  "author": "dino",
  "license": "ISC",
  "nodemonConfig": {
    "exec": "prettier --write src/ | babel-node ./src/bin/server.js",
    "ext": "js,json,hbs,html",
    "watch": [
      "src/*",
      "public/*",
      "views/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/register": "^7.23.7",
    "babel-node": "^0.0.1-security",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-swagger-generator": "^1.1.17",
    "hbs": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "sequelize-paginate": "^1.1.6"
  }
}
